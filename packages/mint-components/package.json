{
  "name": "@saasquatch/mint-components",
  "version": "0.0.1-8",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "module": "dist/index.js",
  "jsnext:main": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build-widget": "stencil build --docs",
    "build:grapes": "cross-env BUILD=DOCS stencil build --watch --docs",
    "serve:grapes": "npx http-server . --cors -c-1",
    "start": "stencil build --dev --watch --serve --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@stencil/core": "^2.4.0",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/utils": "latest",
    "@types/resize-observer-browser": "^0.1.5",
    "clipboard": "^1.7.1",
    "cross-env": "^7.0.2",
    "docx": "^5.4.1",
    "dom-context": "0.0.5",
    "emotion": "^9.2.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "stencil-docx-docs": "0.0.2",
    "ts-node": "^7.0.1",
    "workbox-build": "4.3.1"
  },
  "dependencies": {
    "@saasquatch/component-boilerplate": "^1.0.0-beta10.5",
    "@saasquatch/stencil-hooks": "^2.0.0",
    "@saasquatch/stencilbook": "0.0.1-beta6",
    "@saasquatch/universal-hooks": "^0.1.3",
    "@shoelace-style/shoelace": "^2.0.0-beta.32",
    "@stencil/helmet": "^0.3.2",
    "babel-polyfill": "^6.26.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "highlight.js": "^10.6.0",
    "path-to-regexp": "^6.2.0"
  }
}
